import copy as cp
l1 = [1,2,3,"apple","grapes"]
l2 = copy.copy[l1]
print(l2)
l2[2] = 43
print(l1)
print(l2)
----------------------------------------------
xs = [[1, 2, 3], [4, 5, 6], [7, 8, 9]]
ys = list(xs) 
print(ys)
